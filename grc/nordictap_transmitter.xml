  <callback>transmit($address, $payload, $channel_index, $sequence_number)</callback>
 <param>
   <name>Address</name>
   <key>address</key>
   <value>\x55\x55\x55\x55\x55</value>
   <type>string</type>
 </param>
<param>
   <name>Payload</name>
   <key>payload</key>
   <value>\xAA\xAA\xAA\xAA\xAA</value>
   <type>string</type>
 </param>
 <param>
   <name>Channel Index</name>
   <key>channel_index</key>
   <value>0</value>
   <type>int</type>
 </param>
<param>
   <name>Sequence Number</name>
   <key>sequence_number</key>
   <value>0</value>
   <type>int</type>
 </param>
 <source>
    <name>nordictap_out</name>
     <type>message</type>
 </source>
</block>
